{
    "manifest_version": 2,
    "name": "T3JS devtools",
    "description": "Easily monitor T3 based applications",
    "version": "0.2",
    "background": {
        "scripts": ["background.bundle.js"],
        "persistent": false
    },
    "devtools_page": "devtools.html",
    "permissions": [
        "<all_urls>",
        "tabs"
    ],
    "web_accessible_resources" : ["inject.bundle.js"]
}